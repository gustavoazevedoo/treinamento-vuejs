{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:vue/vue3-essential",
    "airbnb-base",
    "plugin:vue/vue3-recommended",
    "plugin:vue/vue3-strongly-recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "quotes": [
      "error",
      "double"
    ],
    "import/no-extraneous-dependencies": "off",
    "import/prefer-default-export": "off",
    "vue/multi-word-component-names": "off"
  }
}